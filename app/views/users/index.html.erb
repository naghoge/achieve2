<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= profile_img(user) %>
        <%= user.email %>
        <%= render 'follow_form', user: user %>
        <br>
        <%= link_to "ユーザ詳細", user_path(user), class: 'btn btn-default btn-sm btn-primary' %>
      </div>
  <% end %>
</div>